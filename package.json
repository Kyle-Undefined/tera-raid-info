    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.0",
    "prettier-eslint": "^16.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.5.6",
    "webpack": "^5.93.1",
    "webpack-dev-middleware": "^6.1.2"
  }
