{
  "name": "tera-raid-info",
  "version": "3.0.0",
  "description": "Aggregate view of the [Serebii](https://www.serebii.net/) Tera Raid Info for Pokemon Scarlet & Violet.",
  "main": "./src/main.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /tera-raid-info/ && cp ./src/404.html ./dist",
    "lint": "ng lint",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "version:major": "npm --no-git-tag-version version major",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:patch": "npm --no-git-tag-version version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kyle-Undefined/tera-raid-info.git"
  },
  "keywords": [],
  "author": "Kyle Undefined",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kyle-Undefined/tera-raid-info/issues"
  },
  "homepage": "https://github.com/Kyle-Undefined/tera-raid-info#readme",
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@apollo/client": "3.8.8",
    "apollo-angular": "5.0.2",
    "graphql": "16.8.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.11",
    "@angular/cli": "17.3.11",
    "@angular/compiler-cli": "17.3.12",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@favware/graphql-pokemon": "^8.6.1",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
